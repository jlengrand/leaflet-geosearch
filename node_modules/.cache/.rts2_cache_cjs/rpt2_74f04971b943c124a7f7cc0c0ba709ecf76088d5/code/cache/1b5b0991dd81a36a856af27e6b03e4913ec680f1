./providers/provider';\r\ninterface ResultListProps {\r\n    handleClick: (args: {\r\n        result: SearchResult;\r\n    }) => void;\r\n    classNames?: {\r\n        resultlist?: string;\r\n        item?: string;\r\n        notfound?: string;\r\n    };\r\n    notFoundMessage?: string;\r\n}\r\nexport default class ResultList {\r\n    handleClick?: (args: {\r\n        result: SearchResult;\r\n    }) => void;\r\n    selected: number;\r\n    results: SearchResult[];\r\n    container: HTMLDivElement;\r\n    resultItem: HTMLDivElement;\r\n    notFoundMessage?: HTMLDivElement;\r\n    constructor({ handleClick, classNames, notFoundMessage, }: ResultListProps);\r\n    render(results: SearchResult<any>[] | undefined, resultFormat: Function): void;\r\n    select(index: number): SearchResult;\r\n    count(): number;\r\n    clear(): void;\r\n    onClick: (event: Event) => void;\r\n}\r\nexport {};\r\n"}}
