{"code":"import { createElement, addClassName, removeClassName, cx } from './domUtils';\r\nexport default class ResultList {\r\n    constructor({ handleClick, classNames = {}, notFoundMessage, }) {\r\n        this.selected = -1;\r\n        this.results = [];\r\n        this.onClick = (event) => {\r\n            if (typeof this.handleClick !== 'function') {\r\n                return;\r\n            }\r\n            const target = event.target;\r\n            if (!target ||\r\n                !this.container.contains(target) ||\r\n                !target.hasAttribute('data-key')) {\r\n                return;\r\n            }\r\n            const idx = Number(target.getAttribute('data-key'));\r\n            this.clear();\r\n            this.handleClick({ result: this.results[idx] });\r\n        };\r\n        this.handleClick = handleClick;\r\n        this.notFoundMessage = !!notFoundMessage\r\n            ? createElement('div', cx(classNames.notfound), undefined, { html: notFoundMessage })\r\n            : undefined;\r\n        this.container = createElement('div', cx('results', classNames.resultlist));\r\n        this.container.addEventListener('click', this.onClick, true);\r\n        this.resultItem = createElement('div', cx(classNames.item));\r\n    }\r\n    render(results = [], resultFormat) {\r\n        this.clear();\r\n        results.forEach((result, idx) => {\r\n            const child = this.resultItem.cloneNode(true);\r\n            child.setAttribute('data-key', `${idx}`);\r\n            child.innerHTML = resultFormat({ result });\r\n            this.container.appendChild(child);\r\n        });\r\n        if (results.length > 0) {\r\n            addClassName(this.container.parentElement, 'open');\r\n            addClassName(this.container, 'active');\r\n        }\r\n        else if (!!this.notFoundMessage) {\r\n            this.container.appendChild(this.notFoundMessage);\r\n            addClassName(this.container.parentElement, 'open');\r\n        }\r\n        this.results = results;\r\n    }\r\n    select(index) {\r\n        // eslint-disable-next-line no-confusing-arrow\r\n        Array.from(this.container.children).forEach((child, idx) => idx === index\r\n            ? addClassName(child, 'active')\r\n            : removeClassName(child, 'active'));\r\n        this.selected = index;\r\n        return this.results[index];\r\n    }\r\n    count() {\r\n        return this.results ? this.results.length : 0;\r\n    }\r\n    clear() {\r\n        this.selected = -1;\r\n        while (this.container.lastChild) {\r\n            this.container.removeChild(this.container.lastChild);\r\n        }\r\n        removeClassName(this.container.parentElement, 'open');\r\n        removeClassName(this.container, 'active');\r\n    }\r\n}\r\n//# sourceMappingURL=resultList.js.map","references":["/Users/julienlengrand-lambert/Developer/leaflet-geosearch/src/domUtils.ts","/Users/julienlengrand-lambert/Developer/leaflet-geosearch/src/providers/provider.ts"],"map":"{\"version\":3,\"file\":\"resultList.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../src/resultList.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,aAAa,EAAE,YAAY,EAAE,eAAe,EAAE,EAAE,EAAE,MAAM,YAAY,CAAC;AAa9E,MAAM,CAAC,OAAO,OAAO,UAAU;IAS7B,YAAY,EACV,WAAW,EACX,UAAU,GAAG,EAAE,EACf,eAAe,GACC;QAXlB,aAAQ,GAAG,CAAC,CAAC,CAAC;QACd,YAAO,GAAmB,EAAE,CAAC;QA8E7B,YAAO,GAAG,CAAC,KAAY,EAAQ,EAAE;YAC/B,IAAI,OAAO,IAAI,CAAC,WAAW,KAAK,UAAU,EAAE;gBAC1C,OAAO;aACR;YAED,MAAM,MAAM,GAAG,KAAK,CAAC,MAAwB,CAAC;YAC9C,IACE,CAAC,MAAM;gBACP,CAAC,IAAI,CAAC,SAAS,CAAC,QAAQ,CAAC,MAAM,CAAC;gBAChC,CAAC,MAAM,CAAC,YAAY,CAAC,UAAU,CAAC,EAChC;gBACA,OAAO;aACR;YAED,MAAM,GAAG,GAAG,MAAM,CAAC,MAAM,CAAC,YAAY,CAAC,UAAU,CAAC,CAAC,CAAC;YACpD,IAAI,CAAC,KAAK,EAAE,CAAC;YACb,IAAI,CAAC,WAAW,CAAC,EAAE,MAAM,EAAE,IAAI,CAAC,OAAO,CAAC,GAAG,CAAC,EAAE,CAAC,CAAC;QAClD,CAAC,CAAC;QApFA,IAAI,CAAC,WAAW,GAAG,WAAW,CAAC;QAC/B,IAAI,CAAC,eAAe,GAAG,CAAC,CAAC,eAAe;YACtC,CAAC,CAAC,aAAa,CACX,KAAK,EACL,EAAE,CAAC,UAAU,CAAC,QAAQ,CAAC,EACvB,SAAS,EACT,EAAE,